{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "KestrelServer": {
    "Endpoints": {
      "Insecure": {
        "Host": "localhost",
        "Port": 44333,
        "Scheme": "http",
        "Enable": "true"
      },
      "Secure": {
        "Host": "localhost",
        "Port": 52258,
        "Scheme": "https",
        "Enable": "false"
      }
    }
  },
  "FirewallRules": {
    "DnsDynamicAllow": [
      "debian.org",
      "kernel.org",
      "ochap.local",
      "ubuntu.com"
    ],
    "DnsDynamicAllowContains": [
      ".local",
      ".net",
      ".org"
    ],
    "DnsDynamicAllowRegEx": [
      "[a-zA-Z0-9]*\\.local$",
      "[a-zA-Z0-9]*\\.net$"
    ],
    "DnsDynamicDeny": [
      "debian.org",
      "kernel.org",
      "ochap.local",
      "ubuntu.com"
    ],
    "DnsDynamicDenyContains": [
      ".local",
      ".net",
      ".org"
    ],
    "DnsDynamicDenyRegEx": [
      "[a-zA-Z0-9]*\\.local$",
      "[a-zA-Z0-9]*\\.net$"
    ],
    "IpDynamicAllow": [
      "192.168.0.0/16",
      "172.16.0.0/12,10.0.0.0/8",
      "::1"
    ],
    "IpDynamicDeny": [
      "192.168.0.0/16",
      "172.16.0.0/12",
      "10.0.0.0/8",
      "::1"
    ],
    "ScheduleDynamicAllow": [
      "2017:01:01T17:00:00-2018:01:01T08:00:00",
      "2017:01:01T12:00:00-2018:01:01T13:00:00"
    ],
    "ScheduleDynamicDeny": [
      "2017:01:01T17:00:00-2018:01:01T08:00:00",
      "2017:01:01T12:00:00-2018:01:01T13:00:00"
    ]
  }
}